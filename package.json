{
  "name": "nest-template",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"./src/**/*.{tsx,ts}\"",
    "lint-fix": "eslint \"./src/**/*.{tsx,ts}\" --fix",
    "test": "jest --forceExit --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/ormconfig.ts --transaction all",
    "typeorm:generate": "npm run typeorm -- -d ./src/ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "typeorm:revert": "npm run typeorm -- -d ./src/ormconfig.ts migration:revert",
    "sonar": "sonarqube-verify"
  },
  "dependencies": {
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@nestjsx/util": "^5.0.0-alpha.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cron": "^2.2.0",
    "fastify-swagger": "^5.2.0",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.1",
    "typeorm": "^0.3.12",
    "typeorm-transactional": "^0.4.1",
    "uuid": "^9.0.0",
    "oracledb": "file:libs/oracledb",
    "radash": "^10.7.0",
    "firebase-admin": "11.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser":"^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb":"^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "sonarqube-scanner": "^3.0.1",
    "sonarqube-verify": "^1.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/src",
      "node_modules"
    ],
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      ".*/src/.*\\.d\\.ts",
      ".*/src/testUtil/.*"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
