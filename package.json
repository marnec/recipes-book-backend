{
  "name": "nest-template",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "start:prod-tracing": "node --require './dist/tracing.js' dist/main",
    "lint": "eslint \"./src/**/*.{tsx,ts}\"",
    "lint-fix": "eslint \"./src/**/*.{tsx,ts}\" --fix",
    "test": "jest --forceExit --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/ormconfig.ts",
    "typeorm:generate": "npm run typeorm -- -d ./src/ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "typeorm:revert": "npm run typeorm -- -d ./src/ormconfig.ts migration:revert",
    "sonar": "sonarqube-verify"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/typeorm": "9.0.1",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@nestjsx/util": "^5.0.0-alpha.3",
    "@opentelemetry/api": "^1.0.1",
    "@opentelemetry/auto-instrumentations-node": "^0.23.0",
    "@opentelemetry/exporter-collector": "^0.23.0",
    "@opentelemetry/exporter-jaeger": "^0.23.0",
    "@opentelemetry/exporter-prometheus": "^0.23.0",
    "@opentelemetry/exporter-zipkin": "^0.23.0",
    "@opentelemetry/host-metrics": "^0.23.0",
    "@opentelemetry/instrumentation-express": "^0.23.0",
    "@opentelemetry/instrumentation-http": "^0.23.0",
    "@opentelemetry/node": "^0.23.0",
    "@opentelemetry/plugin-http": "^0.18.2",
    "@opentelemetry/sdk-metrics-base": "^0.25.0",
    "@azzurrodigitale/atlassian-common": "1.4.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cron": "^1.8.2",
    "fastify-swagger": "^4.12.6",
    "moment": "^2.29.1",
    "mssql": "^7.2.1",
    "nestjs-i18n": "^8.1.6",
    "opentelemetry-instrumentation-typeorm": "^0.23.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.3.10",
    "typeorm-transactional": "0.3.0",
    "uninstall": "0.0.0",
    "uuid": "^8.3.2",
    "oracledb": "5.5.0",
    "radash": "7.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.31",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser":"^5.40.1",
    "eslint": "8.2.0",
    "eslint-config-airbnb":"19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "sonarqube-scanner": "^2.8.1",
    "sonarqube-verify": "^1.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/src",
      "node_modules"
    ],
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      ".*/src/.*\\.d\\.ts",
      ".*/src/testUtil/.*"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
